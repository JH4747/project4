
<div class="gridster" id="my_div">
    <ul>
        <li class="gridster-cell turquoise" data-row="1" data-col="1" data-sizex="2" data-sizey="2">
          <% @profiles.each do |profile| %>
            <tr>
              <td class="avatar"><%= image_tag profile.avatar_url(:display) %></td>
            </tr>
          <% end %>
        </li>
        <li class="gridster-cell blue" data-row="1" data-col="1" data-sizex="5" data-sizey="2">
          <div class="col-md-12">
            <table class="table profile table-hover">
              <h3>Profile</h3>
              <% @profiles.each do |profile| %>
              <tr>
                <th class="profile">User ID:</th>
                <td><%= profile.user_id%></td>
              </tr>
              <tr>
                <th class="profile">Name:</th>
                <td><%=profile.name%></td>
              </tr>
              <tr>
                <th class="profile">Contact:</th>
                <td><%=profile.contact%></td>
              </tr>
              <tr>
                <th class="profile">Address:</th>
                <td><%=profile.address%></td>
              </tr>
              <tr>
                <td><%= link_to 'Edit', edit_profile_path(profile),class:"btn btn-sm btn-success" %></td>
              </tr>
              <% end %>
            </table>
          </div>
        </li>
        <li class="gridster-cell turquoise" data-row="1" data-col="3" data-sizex="7" data-sizey="2">

          <div class="col-md-12">
            <table class="table table-hover">
              <h3>Reviews</h3>
              <thead>
                <tr>
                  <th>Service ID</th>
                  <th>Types of Services Needed</th>
                  <th>Date</th>
                  <th>Helper ID</th>
                  <th>Rating</th>
                  <th></th>
                  <th></th>
                  <th></th>
                </tr>
              </thead>
              <tbody>
                <% @review.each do |service| %>
                <tr>
                  <td><%= service.id%></td>
                  <td><%= service.task_list %></td>
                  <td><%= service.date_when_seek_help %></td>
                  <td><%= service.helper_id %></td>
                  <td><%= service.rating_for_helper%></td>
                  <td><%= link_to 'View', edit_service_path(service), class:"btn btn-sm btn-success" %></td>
                  <td><%= link_to 'Remove', service, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-sm btn-danger" %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div
        </li>

        <li class="gridster-cell turquoise" data-row="2" data-col="1" data-sizex="7" data-sizey="2">
          <div class="col-md-12">
            <table class="table table-hover">
              <h3>Services Requested</h3>
              <thead>
                <tr>
                  <th>Service ID</th>
                  <th>Types of Services Needed</th>
                  <th>Date</th>
                  <th>Seeker ID</th>
                  <th>Helper ID</th>
                  <th></th>
                  <th></th>
                  <th></th>
                </tr>
              </thead>
              <tbody>
                <% @services.each do |service| %>
                <tr>
                  <td><%= service.id%></td>
                  <td><%= service.task_list %></td>
                  <td><%= service.date_when_seek_help %></td>
                  <td><%= link_to 'Match', service, class:"btn btn-sm btn-info" %></td>
                  <td><%= link_to 'Edit', edit_service_path(service), class:"btn btn-sm btn-success"%></td>
                  <td><%= link_to 'Remove', service, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-sm btn-danger" %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div
        </li>



        <li class="gridster-cell blue" data-row="3" data-col="1" data-sizex="7" data-sizey="2">
          <div class="col-md-12">
            <table class="table table-hover">
              <h3>Services Offered</h3>
              <thead>
                <tr>
                  <th>Service ID</th>
                  <th>Types of Services Offered</th>
                  <th>Date</th>
                  <th></th>
                  <th></th>
                  <th></th>
                </tr>
              </thead>
              <tbody>
                <% @services1.each do |service| %>
                <tr>
                  <td><%= service.id%></td>
                  <td><%= service.task_list %></td>
                  <td><%= service.date_when_seek_help %></td>
                  <!-- <td><%= link_to 'Match', service, class:"btn btn-sm btn-info" %></td> -->
                  <td><%= link_to 'Edit', edit_service_path(service), class:"btn btn-sm btn-success" %></td>
                  <td><%= link_to 'Remove', service, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-sm btn-danger" %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </li>
    </ul>

</div>
